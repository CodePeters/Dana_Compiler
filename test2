def solve
begin
	def hanoi: rings as int, source as byte [12]
	begin
		def move: source as byte [11]
		begin
			writeString: "Moving From"
			writeString: source
			writeString: " to "
			writeString: target
			writeString: ".\n"
		end
	end
	if rings >= 1:
	begin
		hanoi: rings-1, source, auxiliary, target
		move: source, target
		hanoi: rings-1, auxiliary, target, source
	end
	var numberOfRings is int
	writeString: "Rings: "
	numberOfRings := readInteger
	hanoi: numberOfRings, "left","right","middle"
end